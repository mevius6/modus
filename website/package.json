{
  "type": "module",
  "name": "modus",
  "version": "1.0.0",
  "description": "",
  "browserslist": "> 0.5%, last 2 versions, not dead, not op_mini all",
  "config": {
    "repo": "https://github.com/mevius6/${npm_package_name}",
    "https": {
      "cert": "$(grep '^CERT_PATH' .env | cut -d '=' -f2)",
      "key": "$(grep '^KEY_PATH' .env | cut -d '=' -f2)"
    },
    "port": {
      "http": 8080,
      "https": 8443
    }
  },
  "scripts": {
    "start": "vite --host",
    "build": "vite build --base=./",
    "clean": "rm -rf dist && rm -rf output",
    "test": "vitest",
    "copy": "cp -r src/static/* ./public"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cssnano": "^7.0.4",
    "eslint": "^9.5.0",
    "postcss": "^8.4.39",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^9.5.16",
    "stylelint-order": "^6.0.4",
    "vite": "^5.3.2"
  }
}
